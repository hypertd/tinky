#!/usr/bin/env php
<?php
$bootConfig  = (include __DIR__.'/Config.php');

if($bootConfig['psyshRootLocation'] && file_exists($bootConfig['psyshRootLocation'])){
	require_once $bootConfig['psyshRootLocation'];
}
else{
	echo 'There was a problem loading the PsySH library, check path or config';
	exit;
}

require __DIR__.'/Start.php';